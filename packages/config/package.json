{
  "name": "@naiable/rollup-config",
  "version": "0.1.3",
  "description": "Use rollup like tsup, but opinionated my way.",
  "author": {
    "name": "Naily Zero",
    "email": "zero@naily.cc",
    "url": "https://github.com/nailiable"
  },
  "license": "MIT",
  "homepage": "https://github.com/nailiable",
  "repository": {
    "type": "git",
    "url": "git+github.com:nailiable/rollup-config.git",
    "directory": "packages/rollup-config"
  },
  "bugs": {
    "url": "https://github.com/nailiable/rollup-config/issues",
    "email": "zero@naily.cc"
  },
  "keywords": [
    "rollup",
    "tsup",
    "swc",
    "typescript",
    "rollup-plugin-swc",
    "rollup-plugin-typescript",
    "rollup-plugin-node-resolve",
    "rollup-plugin-commonjs",
    "rollup-plugin-alias",
    "rollup-plugin-dts",
    "rollup-pluginutils"
  ],
  "exports": {
    ".": {
      "types": {
        "import": "./dist/esm/index.d.mts",
        "require": "./dist/cjs/index.d.cts",
        "default": "./dist/types/index.d.mts"
      },
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "bin": {
    "naiup": "bin/naiup"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-swc",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "tag": "latest"
  },
  "dependencies": {
    "@rollup/plugin-alias": "5.1.0",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-swc": "0.3.0",
    "@rollup/plugin-typescript": "11.1.6",
    "@rollup/pluginutils": "5.1.0",
    "@swc/core": "1.5.0",
    "defu": "6.1.4",
    "rollup": "4.16.4",
    "rollup-plugin-dts": "6.1.0"
  }
}
